<%if post.errors.messages.any?%>
<ul>
      <%=post.errors.full_messages do |msg|%>
      <li>
            <%=msg%>
      </li>
      <%end%>
</ul>
<%end%>
<%= form_for post do |p| %>

<div class="add_title">
      <%=p.label :title%>
      <%=p.text_field :title%>
</div>
<div class="add_post">
      <%=p.label :content%>
      <%=p.rich_text_area :content%>
</div>
<%=p.submit%>


<%end%>


